package it.polito.oop.vaccination;

public class Person {
	private String first;
	private String last;
	private String ssn;
	int birthyear;

	
	public Person(String first, String last, String ssn, int birthyear) {
		
		this.first = first;
		this.last = last;
		this.ssn = ssn;
		this.birthyear = birthyear;
	}
	String getFirst() {
		return first;
	}
	String getLast() {
		return last;
	}
	String getSsn() {
		return ssn;
	}
	int getBirthyear() {
		return birthyear;
	}
	@Override
	public String toString() {
		return ssn +","+ first+ "," + last;
	};
	
	
	
}
